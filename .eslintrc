{
  "extends": [
    "standard",
    "plugin:vue/recommended"
  ],
  "plugins": [
    "import",
    "html"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "semi": "off",
    "indent": "off", // 不检查缩进
    "vue/script-indent": "off",
    "vue/html-self-closing": "off"
  }
}
